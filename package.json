{
  "name": "spy4js",
  "version": "2.11.0",
  "description": "Smart, compact and powerful spy test framework",
  "main": "dist/cjs/spy.js",
  "module": "dist/esm/spy.js",
  "types": "src/spy.d.ts",
  "files": [
    "dist/cjs",
    "dist/esm",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fdc-viktor-luft/spy4js"
  },
  "keywords": [
    "spy",
    "test",
    "tool",
    "jest",
    "flow",
    "TypeScript"
  ],
  "author": "Viktor Luft <viktor.luft@freiheit.com> (https://github.com/fdc-viktor-luft)",
  "license": "MIT",
  "scripts": {
    "dist": "rm -rf dist && rollup -c",
    "eslint": "eslint --cache --cache-location 'build/.eslintcache' src/ test/",
    "flow": "flow --quiet",
    "test": "npm run flow && npm run eslint && npm run test-coverage",
    "test-coverage": "jest --all --coverage",
    "prettier": "prettier --config .prettierrc --write src/**/*.js test/**/*.js",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "serialize-as-code": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "coveralls": "^3.0.7",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "flow-bin": "^0.111.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "rollup": "^1.26.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-flow-entry": "^0.3.3",
    "rollup-plugin-node-resolve": "^5.2.0"
  }
}
